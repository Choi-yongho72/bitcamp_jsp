<body>
<script type="text/javascript">

var colors = ['red', 'blue', 'green'];
var me = {
		'name': 'young',
		'birth': 1996,
		'sex': 'm'
}

document.write('<h3>colors</h3>');

for(var i in colors)
	document.write( i + ': ' + colors[i] + '<br>');
	
document.write('<h3>me</h3>');

for (var i in me)
	document.write( i + ': ' + me[i] + '<br>');
</script>
